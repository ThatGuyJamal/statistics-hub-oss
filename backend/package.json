{
  "name": "@statistics-hub-oss/core",
  "version": "0.1.0",
  "description": "A discord bot for tracking statistics in your server.",
  "main": "out/registry.js",
  "scripts": {
    "pm2": "pm2 start pm2.ecosystem.yml",
    "compile": "tsc -p tsconfig.json",
    "start:nodemon": "nodemon ./out/registry.js",
    "start:development": "concurrently \"yarn ts:watch\" \"yarn start\"",
    "ts:watch": "tsc --watch",
    "start:production": "node out/registry.js --trace-warnings",
    "format": "prettier --ignore-path .gitignore --write .",
    "start": "node ./out/registry.js --trace-warnings"
  },
  "keywords": [
    "discord-bot",
    "discord.js",
    "statistics",
    "server",
    "bot",
    "discord",
    "typescript"
  ],
  "author": "ThatGuyJamal",
  "license": "ISC",
  "homepage": "https://github.com/ThatGuyJamal/statistics-hub-oss",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThatGuyJamal/statistics-hub-oss.git"
  },
  "bugs": {
    "url": "https://github.com/ThatGuyJamal/statistics-hub-oss/issues"
  },
  "dependencies": {
    "@discordjs/collection": "^0.6.0",
    "consola": "^2.15.3",
    "discord-welcome-card": "^4.7.0",
    "discordeno": "^13.0.0-rc35",
    "min-indent": "^1.0.1",
    "mongoose": "^6.3.2",
    "ms": "^2.1.3"
  },
  "devDependencies": {
    "@types/min-indent": "^1.0.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.18",
    "concurrently": "^7.0.0",
    "pm2": "^5.1.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=17.0",
    "npm": ">=7"
  },
  "packageManager": "yarn@3.1.1"
}
