{
  "name": "@statistics-hub-oss/core",
  "version": "0.1.0",
  "description": "A discord bot for tracking statistics in your server.",
  "main": "out/registry.js",
  "scripts": {
    "pm2": "pm2 start pm2.ecosystem.yml",
    "compile": "tsc -p tsconfig.json",
    "start:nodemon": "nodemon ./out/registry.js",
    "start:development": "concurrently \"yarn ts:watch\" \"yarn start:production\"",
    "ts:watch": "tsc --watch",
    "start:production": "node out/registry.js",
    "check:format": "prettier --ignore-path .gitignore --write .",
    "start": "node ./out/registry.js"
  },
  "keywords": [
    "discord-bot",
    "discord.js",
    "statistics",
    "server",
    "bot",
    "discord",
    "typescript"
  ],
  "author": "ThatGuyJamal",
  "license": "GNU",
  "repository": {
    "type": "git",
    "url": "https://github.com/ThatGuyJamal/statshub-oss.git"
  },
  "homepage": "https://github.com/ThatGuyJamal/statshub-oss",
  "bugs": {
    "url": "https://github.com/ThatGuyJamal/statshub-oss/issues"
  },
  "dependencies": {
    "@kaname-png/plugin-statcord": "^1.1.3",
    "@sapphire/decorators": "^4.2.3",
    "@sapphire/framework": "^3.0.0-next.73cfe9f.0",
    "@sapphire/plugin-editable-commands": "^1.1.3",
    "@sapphire/plugin-hmr": "^1.1.1",
    "@sapphire/plugin-i18next": "^2.2.3",
    "@sapphire/plugin-logger": "^2.1.2",
    "@sapphire/plugin-scheduled-tasks": "^2.2.0",
    "@sapphire/plugin-subcommands": "^2.1.3",
    "@sapphire/ratelimits": "^2.1.12",
    "@sapphire/time-utilities": "^1.5.2",
    "@sapphire/type": "^2.1.2",
    "@typegoose/typegoose": "^9.7.0",
    "discord.js": "^13.6.0",
    "discord.js-docs": "^0.3.0",
    "fuse.js": "^6.5.3",
    "mongoose": "^6.2.10",
    "ms": "^2.1.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.18",
    "concurrently": "^7.0.0",
    "pm2": "^5.1.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=17.0",
    "npm": ">=7"
  },
  "packageManager": "yarn@3.1.1"
}
